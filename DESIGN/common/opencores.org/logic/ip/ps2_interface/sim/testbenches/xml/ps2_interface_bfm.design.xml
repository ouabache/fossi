    </ipxact:adHocConnection>

 <ipxact:adHocConnection>
      <ipxact:name>busy</ipxact:name>
      <ipxact:internalPortReference ipxact:componentRef="ps2_host" ipxact:portRef="busy"/>
    </ipxact:adHocConnection>

 <ipxact:adHocConnection>
      <ipxact:name>rx_clear</ipxact:name>
      <ipxact:internalPortReference ipxact:componentRef="ps2_host" ipxact:portRef="rx_clr"/>
    </ipxact:adHocConnection>


 <ipxact:adHocConnection>
      <ipxact:name>rx_data</ipxact:name>
      <ipxact:internalPortReference ipxact:componentRef="ps2_host" ipxact:portRef="rx_data"/>
    </ipxact:adHocConnection>


 <ipxact:adHocConnection>
      <ipxact:name>rx_read</ipxact:name>
      <ipxact:internalPortReference ipxact:componentRef="ps2_host" ipxact:portRef="rx_read"/>
    </ipxact:adHocConnection>


 <ipxact:adHocConnection>
      <ipxact:name>rx_full</ipxact:name>
      <ipxact:internalPortReference ipxact:componentRef="ps2_host" ipxact:portRef="rx_full"/>
    </ipxact:adHocConnection>

 <ipxact:adHocConnection>
      <ipxact:name>rx_parity_error</ipxact:name>
      <ipxact:internalPortReference ipxact:componentRef="ps2_host" ipxact:portRef="rx_parity_error"/>
    </ipxact:adHocConnection>

 <ipxact:adHocConnection>
      <ipxact:name>rx_parity_rcv</ipxact:name>
      <ipxact:internalPortReference ipxact:componentRef="ps2_host" ipxact:portRef="rx_parity_rcv"/>
    </ipxact:adHocConnection>

 <ipxact:adHocConnection>
      <ipxact:name>rx_parity_cal</ipxact:name>
      <ipxact:internalPortReference ipxact:componentRef="ps2_host" ipxact:portRef="rx_parity_cal"/>
    </ipxact:adHocConnection>

 <ipxact:adHocConnection>
      <ipxact:name>rx_frame_error</ipxact:name>
      <ipxact:internalPortReference ipxact:componentRef="ps2_host" ipxact:portRef="rx_frame_error"/>
    </ipxact:adHocConnection>


 <ipxact:adHocConnection>
      <ipxact:name>tx_ack_error</ipxact:name>
      <ipxact:internalPortReference ipxact:componentRef="ps2_host" ipxact:portRef="tx_ack_error"/>
    </ipxact:adHocConnection>

 <ipxact:adHocConnection>
      <ipxact:name>tx_data</ipxact:name>
      <ipxact:internalPortReference ipxact:componentRef="ps2_host" ipxact:portRef="tx_data"/>
    </ipxact:adHocConnection>


 <ipxact:adHocConnection>
      <ipxact:name>tx_write</ipxact:name>
      <ipxact:internalPortReference ipxact:componentRef="ps2_host" ipxact:portRef="tx_write"/>
    </ipxact:adHocConnection>



</ipxact:adHocConnections>



<ipxact:componentInstances>




<ipxact:componentInstance>
<ipxact:instanceName>ps2_model</ipxact:instanceName> <ipxact:componentRef vendor="opencores.org" library="Testbench" name="ps2_model" version="def" />
<ipxact:configurableElementValues>
 <ipxact:configurableElementValue ipxact:referenceId="CLKCNT">PS2_MODEL_CLKCNT</ipxact:configurableElementValue>
</ipxact:configurableElementValues>

</ipxact:componentInstance>   





<ipxact:componentInstance>
<ipxact:instanceName>data_tri_buf</ipxact:instanceName> <ipxact:componentRef vendor="opencores.org" library="cde" name="pad" version="od_dig" />
</ipxact:componentInstance>   

<ipxact:componentInstance>
<ipxact:instanceName>clk_tri_buf</ipxact:instanceName> <ipxact:componentRef vendor="opencores.org" library="cde" name="pad" version="od_dig" />
</ipxact:componentInstance>   


<ipxact:componentInstance>
<ipxact:instanceName>ps2_host</ipxact:instanceName> <ipxact:componentRef vendor="opencores.org" library="Testbench" name="ps2_host" version="def" />
</ipxact:componentInstance>   



</ipxact:componentInstances>








