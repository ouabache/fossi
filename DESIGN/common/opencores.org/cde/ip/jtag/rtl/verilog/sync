   assign    syn_clk             = clk;
   assign    syn_select          = select;
   assign    syn_tdo             = tdi;
   assign    tdo                 = syn_tdi;   
   assign    syn_capture_dr      = capture_dr;   
   assign    syn_shift_dr        = shift_dr  ;
   assign    syn_update_dr       = update_dr;
