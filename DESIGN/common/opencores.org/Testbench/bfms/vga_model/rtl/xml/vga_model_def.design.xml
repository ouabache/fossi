      <ipxact:externalPortReference ipxact:portRef="mask_device_rx_parity"        />
      <ipxact:internalPortReference ipxact:componentRef="device_rx_parity_tpb" ipxact:portRef="mask"/>
    </ipxact:adHocConnection>





    <ipxact:adHocConnection>
      <ipxact:name>prb_device_rx_data</ipxact:name>
      <ipxact:externalPortReference ipxact:portRef="prb_device_rx_data"   ipxact:left="7"   ipxact:right="0"     />
      <ipxact:internalPortReference ipxact:componentRef="device_rx_data_tpb" ipxact:portRef="signal"/>
    </ipxact:adHocConnection>


    <ipxact:adHocConnection>
      <ipxact:name>prb_device_rx_parity</ipxact:name>
      <ipxact:externalPortReference ipxact:portRef="prb_device_rx_parity"        />
      <ipxact:internalPortReference ipxact:componentRef="device_rx_parity_tpb" ipxact:portRef="signal"/>
    </ipxact:adHocConnection>



</ipxact:adHocConnections>



