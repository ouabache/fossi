      <ipxact:internalPortReference ipxact:componentRef="device_rx_parity_tpb" ipxact:portRef="clk"/>
    </ipxact:adHocConnection>



    <ipxact:adHocConnection>
      <ipxact:name>exp_device_rx_data</ipxact:name>
